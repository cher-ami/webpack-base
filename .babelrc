{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current",
          "browsers": "> 0.25%, last 2 versions, not dead"
        }
      }
    ],
    [
      "@babel/preset-typescript",
      {
        "allowDeclareFields": true
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": ["import-glob-array", "@babel/plugin-transform-runtime"]
}
