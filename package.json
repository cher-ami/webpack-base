{
  "name": "webpack-base",
  "version": "1.0.0",
  "description": "Cher Ami boilerplate for javascript projects",
  "author": "Willy Brauner & Cher Ami",
  "license": "MIT",
  "main": "src/index.tsx",
  "keywords": [
    "webpack",
    "boilerplate",
    "react",
    "typescript",
    "less"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/cher-ami/webpack-base.git"
  },
  "scripts": {
    "dev": "NODE_ENV=development env-cmd --no-override -f .env node config/tasks/runner.js dev",
    "build": "NODE_ENV=production env-cmd --no-override -f .env.production --fallback node config/tasks/runner.js build",
    "clean": "node config/tasks/runner.js clean",
    "reset": "node config/tasks/runner.js reset",
    "scaffold": "node config/tasks/runner.js scaffold",
    "setup": "npm i && node config/tasks/runner.js setup && npm run prepare",
    "help": "node config/tasks/runner.js help",
    "prettier": "prettier --write .",
    "test": "jest src/*",
    "test-framework": "jest config/*",
    "postinstall": "cp -n .env.example .env || true",
    "prepare": "husky install"
  },
  "dependencies": {
    "@cher-ami/router": "^1.0.0-alpha.7",
    "@solid-js/css-flat": "^1.0.1",
    "@wbe/deferred-promise": "^1.0.3",
    "@wbe/libraries": "^1.6.0",
    "gsap": "^3.6.1",
    "preact": "^10.5.13",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.14.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@solid-js/cli": "^1.5.1",
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.2",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.4",
    "@zouloux/files": "^0.6.0",
    "app-root-path": "^3.0.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-import-glob-array": "^0.2.0",
    "change-case": "^4.1.2",
    "colors": "^1.4.0",
    "copy-webpack-plugin": "^8.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "cssnano": "^5.0.2",
    "debug": "^4.3.1",
    "dotenv-webpack": "^7.0.2",
    "env-cmd": "^10.1.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.6",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "inquirer": "^8.0.0",
    "internal-ip": "^6.2.0",
    "jest": "^26.6.3",
    "less": "^4.1.1",
    "less-loader": "^8.1.1",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "portfinder-sync": "^0.0.2",
    "postcss-loader": "^5.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.0",
    "raw-loader": "^4.0.2",
    "react-refresh": "^0.10.0",
    "react-test-renderer": "^17.0.2",
    "style-loader": "^2.0.0",
    "typescript": "^4.2.4",
    "webpack": "^5.37.0",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-manifest-plugin": "^3.1.1",
    "webpack-merge": "^5.7.3"
  },
  "lint-staged": {
    "*.{tsx,ts,jsx,js,less,css,json,md}": [
      "prettier --write"
    ]
  }
}
