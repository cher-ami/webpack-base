{
  "name": "webpack-base",
  "version": "1.2.0",
  "description": "Cher Ami boilerplate for javascript projects",
  "author": "Cher Ami",
  "license": "MIT",
  "main": "src/index.tsx",
  "keywords": [
    "webpack",
    "boilerplate",
    "react",
    "typescript",
    "less"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/cher-ami/webpack-base.git"
  },
  "scripts": {
    "dev": "NODE_ENV=development env-cmd --no-override -f .env node config/tasks/runner.js dev",
    "build": "NODE_ENV=production node config/tasks/runner.js build",
    "clean": "node config/tasks/runner.js clean",
    "reset": "node config/tasks/runner.js reset",
    "scaffold": "node config/tasks/runner.js scaffold",
    "wp-scaffold": "node config/tasks/runner.js wp-scaffold",
    "setup": "npm i && node config/tasks/runner.js setup && npm run prepare",
    "help": "node config/tasks/runner.js help",
    "prettier": "prettier --write .",
    "test": "jest src/*",
    "test-framework": "jest config/*",
    "postinstall": "cp -n .env.example .env || true",
    "prepare": "husky install"
  },
  "dependencies": {
    "@cher-ami/router": "^1.0.0",
    "@solid-js/css-flat": "^1.0.1",
    "gsap": "^3.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.15.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@solid-js/cli": "^1.6.3",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.6",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@zouloux/files": "^0.6.0",
    "app-root-path": "^3.0.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-import-glob-array": "^0.2.0",
    "change-case": "^4.1.2",
    "colors": "^1.4.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "cssnano": "^5.0.8",
    "debug": "^4.3.2",
    "dotenv-webpack": "^7.0.3",
    "env-cmd": "^10.1.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.3.2",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.2",
    "inquirer": "^8.1.2",
    "internal-ip": "^6.2.0",
    "jest": "^27.1.0",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "lint-staged": "^11.1.2",
    "mini-css-extract-plugin": "^2.2.0",
    "portfinder-sync": "^0.0.2",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.2",
    "raw-loader": "^4.0.2",
    "react-refresh": "^0.10.0",
    "react-test-renderer": "^17.0.2",
    "style-loader": "^3.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2",
    "webpack": "^5.51.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.0.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-manifest-plugin": "^4.0.2",
    "webpack-merge": "^5.8.0"
  },
  "lint-staged": {
    "*.{tsx,ts,jsx,js,less,css,json,md}": [
      "prettier --write"
    ]
  }
}
